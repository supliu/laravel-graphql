{
    "name": "supliu/laravel-graphql",
    "description": "GraphQL with Laravel Framework",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel", "graphql", "api"
    ],
    "authors": [
        {
            "name": "Jansen Felipe",
            "email": "jansen.felipe@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Supliu\\LaravelGraphQL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Supliu\\LaravelGraphQL\\Tests\\": "tests/"
        }
    },
    "require": {
        "webonyx/graphql-php": "14.*",
        "laravel/framework": "^7.30.4|^8.40"
    },
    "require-dev": {
        "orchestra/testbench": "5.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Supliu\\LaravelGraphQL\\ServiceProvider"
            ]
        }
    },
    "suggest": {
        "supliu/laravel-query-monitor": "Simple artisan command to monitoring triggered queries SQL"
    },
    "minimum-stability": "dev"
}
